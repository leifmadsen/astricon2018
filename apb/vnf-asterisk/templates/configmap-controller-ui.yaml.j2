---
apiVersion: v1
kind: ConfigMap
metadata:
  creationTimestamp: null
  name: controller-ui-config-{{ uniqname }}
  namespace: {{ namespace | string }}
  labels:
    app: {{ app_name }}
    service: {{ app_name }}
    service_class_id: '{{ _apb_service_class_id }}'
    plan_id: '{{ _apb_plan_id }}'
    service_instance_id: '{{ _apb_service_instance_id }}'
data:
  config.js: |
    var env = {
      environment_name: "kubernetes_config",
      api_url: 'http://controller-{{ uniqname }}:8001',
    };

