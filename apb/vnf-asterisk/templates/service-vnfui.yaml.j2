---
apiVersion: v1
kind: Service
metadata:
  creationTimestamp: null
  labels:
    app: {{ app_name }}
    service: {{ app_name }}
    service_class_id: '{{ _apb_service_class_id }}'
    plan_id: '{{ _apb_plan_id }}'
    service_instance_id: '{{ _apb_service_instance_id }}'
  name: vnfui-{{ uniqname }}
  namespace: {{ namespace | string }}
spec:
  # clusterIP: None
  ports:
  - name: "80"
    port: 80
    protocol: TCP
    targetPort: 80
  selector:
    app_selector: vnfui-{{ uniqname }}
status:
  loadBalancer: {}
